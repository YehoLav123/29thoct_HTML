@page "/table"
<title>MultiplicationTable</title>
<h3>Table</h3>
<link rel="stylesheet" href="MyStyle.css">
<table class="table table-bordered" style="width: 50%; margin: 0 auto;">
        <tr>
            <th>X</th>
            @for (int i = 1; i <= 10; i++)
            {
                <th>@i</th>
            }
        </tr>
        @for (int i = 1; i <= 10; i++)
        {
            <tr>
                <td>@i</td>
                @for (int j = 1; j <= 10; j++)
                {
                    <td>@(i * j)</td>
                }
            </tr>
        }
</table>
